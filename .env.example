# 应用配置
APP_NAME=sia-image-service
APP_VERSION=1.0.0
APP_ENVIRONMENT=development

# 服务器配置
GRPC_PORT=8080
HTTP_PORT=9090

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=json

# 图片生成API配置
IMAGE_API_KEY=your_api_key_here
IMAGE_BASE_URL=https://ark.cn-beijing.volces.com
IMAGE_MODEL=doubao-seedream-4-0-250828
IMAGE_DEFAULT_SIZE=2K
IMAGE_TIMEOUT=300
IMAGE_MAX_RETRIES=3